    "cors": "^2.8.5",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.30.1",
    "express": "^4.18.1",
    "filesize": "^9.0.11",
    "framer-motion": "^6",
    "fuzzyset.js": "^1.0.7",
    "http-proxy-middleware": "^2.0.6",
    "iso-639-1": "^2.1.15",
    "js-cookie": "^3.0.1",
    "morgan": "^1.10.0",
    "moviedb-promise": "^3.1.18",
    "prettier": "^2.6.0",
    "react": "18.2.0",
    "react-activity-rings": "^1.1.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.2.0",
    "react-ga": "^3.3.1",
    "react-icons": "^4.3.1",
    "react-query": "^3.39.3",
